{
  "name": "error-mask",
  "version": "1.0.13",
  "description": "A light weight package to get the appropriate error message for every error.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc"
  },
  "author": "Shivam-Saksham",
  "license": "ISC",
  "devDependencies": {
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "keywords": [
    "error"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shivamsaksham/error-mask"
  }
}
