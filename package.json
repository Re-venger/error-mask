{
  "name": "error-mask",
  "version": "1.0.8",
  "description": "A light weight package to get the appropriate error message for every error.",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json"
  },
  "author": "Shivam-Saksham",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.2.5",
    "typescript": "^5.1.3"
  },
  "keywords": [
    "error"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shivamsaksham/error-mask"
  }
}
